{
  "version": 3,
  "configurePresets": [
    {
      "name": "ucrt64-gcc-debug",
       "displayName": "GCC Debug (ucrt64)",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "C:/msys64/ucrt64/bin/cc.exe",
        "CMAKE_CXX_COMPILER": "C:/msys64/ucrt64/bin/g++.exe",
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "ucrt64-gcc-release",
      "displayName": "GCC Release (ucrt64)",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/release",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "C:/msys64/ucrt64/bin/cc.exe",
        "CMAKE_CXX_COMPILER": "C:/msys64/ucrt64/bin/g++.exe",
        "CMAKE_BUILD_TYPE": "Release"
      }
    }

  ],
  "buildPresets": [
    {
      "name": "gcc-debug",
      "configurePreset": "ucrt64-gcc-debug",
      "targets": ["wxTranslator"]
    },
    {
      "name": "gcc-release",
      "configurePreset": "ucrt64-gcc-release",
      "targets": ["wxTranslator"]
    }
  ]
}
